    "babel-preset-react": "^6.24.1",
    "html-webpack-plugin": "^5.5.0",
    "http-server": "^0.11.1",
    "npm-run-all": "^4.1.3",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1",
    "webpack-dev-server": "^3.9.0"
  }
}
